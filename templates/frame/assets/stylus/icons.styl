@font-face {
	font-family: 'frame';
	src:url('../fonts/frame.eot?55ya0r');
	src:url('../fonts/frame.eot?#iefix55ya0r') format('embedded-opentype'),
		url('../fonts/frame.woff?55ya0r') format('woff'),
		url('../fonts/frame.ttf?55ya0r') format('truetype'),
		url('../fonts/frame.svg?55ya0r#frame') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'frame';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

